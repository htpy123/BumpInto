<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="bump">
	
	<select id="getList" resultType="BumpDto">
		SELECT *
		FROM BumpInto
		ORDER BY num ASC
	</select>
	
	<insert id="addBump" parameterType="BumpDto">
		INSERT INTO BumpInto
		(num,writer,title,content,regdate,aptdate)
		VALUES(BumpInto_seq.NEXTVAL, #{wrtier}, #{title}, #{content},SYSDATE,#{aptdate})
	</insert>
	
	<update id="update">
		UPDATE BumpInto
		SET title
		WHERE 
	</update>
</mapper>